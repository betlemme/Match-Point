package it.gemmed.resource;

import java.sql.Date;
import java.sql.Time;


/**
 * Rappresenta una partita singola
 * @author manuel
 *
 */
public class PartitaDoppia {
	private final int numeroPartita;
	private Date data;
	private Time ora;
	private int campo;
	private String circolo;
	private String teamA;
	private String teamB;
	private boolean vittoriaTeamA;
	private Risultato set1;
	private Risultato set2;
	private Risultato set3;
	private final int torneo;
	
	/**
	 * @param numeroPartita
	 * @param torneo
	 */
	public PartitaDoppia(int numeroPartita, int torneo) {
		super();
		this.numeroPartita = numeroPartita;
		this.torneo = torneo;
	}

	/**
	 * @return the data
	 */
	public Date getData() {
		return data;
	}

	/**
	 * @param data the data to set
	 */
	public void setData(Date data) {
		this.data = data;
	}

	/**
	 * @return the ora
	 */
	public Time getOra() {
		return ora;
	}

	/**
	 * @param ora the ora to set
	 */
	public void setOra(Time ora) {
		this.ora = ora;
	}

	/**
	 * @return the campo
	 */
	public int getCampo() {
		return campo;
	}

	/**
	 * @param campo the campo to set
	 */
	public void setCampo(int campo) {
		this.campo = campo;
	}

	/**
	 * @return the circolo
	 */
	public String getCircolo() {
		return circolo;
	}

	/**
	 * @param circolo the circolo to set
	 */
	public void setCircolo(String circolo) {
		this.circolo = circolo;
	}

	/**
	 * @return the set1
	 */
	public Risultato getSet1() {
		return set1;
	}

	/**
	 * @param set1 the set1 to set
	 */
	public void setSet1(Risultato set1) {
		this.set1 = set1;
	}

	/**
	 * @return the set2
	 */
	public Risultato getSet2() {
		return set2;
	}

	/**
	 * @param set2 the set2 to set
	 */
	public void setSet2(Risultato set2) {
		this.set2 = set2;
	}

	/**
	 * @return the set3
	 */
	public Risultato getSet3() {
		return set3;
	}

	/**
	 * @param set3 the set3 to set
	 */
	public void setSet3(Risultato set3) {
		this.set3 = set3;
	}

	/**
	 * @return the numeroPartita
	 */
	public int getNumeroPartita() {
		return numeroPartita;
	}

	/**
	 * @return the torneo
	 */
	public int getTorneo() {
		return torneo;
	}

	/**
	 * @return the teamA
	 */
	public String getTeamA() {
		return teamA;
	}

	/**
	 * @param teamA the teamA to set
	 */
	public void setTeamA(String teamA) {
		this.teamA = teamA;
	}

	/**
	 * @return the teamB
	 */
	public String getTeamB() {
		return teamB;
	}

	/**
	 * @param teamB the teamB to set
	 */
	public void setTeamB(String teamB) {
		this.teamB = teamB;
	}

	/**
	 * @return the vittoriaTeamA
	 */
	public boolean isVittoriaTeamA() {
		return vittoriaTeamA;
	}

	/**
	 * @param vittoriaTeamA the vittoriaTeamA to set
	 */
	public void setVittoriaTeamA(boolean vittoriaTeamA) {
		this.vittoriaTeamA = vittoriaTeamA;
	}
	
	

}
