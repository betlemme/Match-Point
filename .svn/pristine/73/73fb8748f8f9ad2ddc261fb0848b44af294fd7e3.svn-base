package it.gemmed.resource;

/**
 * Rappresenta una squadra di due giocatori
 * 
 * @author GEMMED
 *
 */
public class Team {
	private final String idteam;
	private final String giocatoreA;
	private final String giocatoreB;
	
	/**
	 * Costruttore completo
	 * 
	 * @param idteam
	 * @param giocatoreA
	 * @param giocatoreB
	 */
	public Team(String idteam, String giocatoreA, String giocatoreB) {
		super();
		this.idteam = idteam;
		this.giocatoreA = giocatoreA;
		this.giocatoreB = giocatoreB;
	}

	/**
	 * @return idteam
	 */
	public String getIdteam() {
		return idteam;
	}

	/**
	 * @return giocatoreA
	 */
	public String getGiocatoreA() {
		return giocatoreA;
	}

	/**
	 * @return giocatoreB
	 */
	public String getGiocatoreB() {
		return giocatoreB;
	}
	
	
}
