package it.gemmed.resource;

/**
 * Rappresenta i dati di un utente federale, nel nostro
 * caso si limitano ad una password ed una email.
 * 
 * @author GEMMED
 */

public class UtenteFederale {
	private final String email;
	private String password;
	
	/**
	 * Costruttore completo
	 * 
	 * @param email
	 * @param password
	 */
	public UtenteFederale(String email, String password) {
		super();
		this.email = email;
		this.password = password;
	}

	/**
	 * @return email
	 */
	public String getEmail() {
		return email;
	}

	/**
	 * @return password
	 */
	public String getPassword() {
		return password;
	}
	/**
	 * @param password the password to set
	 */
	public void setPassword(String password) {
		this.password = password;
	}

}
